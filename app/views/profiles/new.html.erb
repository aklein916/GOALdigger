<h2>New Profile</h2>

<%= form_for @profile do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.text_field :age %>
  
  <%= f.label :date %>
  <%= f.text_field :creationdate %>

  <%= f.label :photo_URL %>
  <%= f.text_field :photo_url %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :favorite_quote %>
  <%= f.text_area :quote %>

  <%= f.submit %>
<% end %>
